@model ExpertransDaoTao.ViewModel.CourseViewModel
@using Newtonsoft.Json
@using ExpertransDaoTao.Models
@using System.Security.Claims
@{
    Layout = "_LayoutStudent";
    ViewData["Title"] = "Học Viên - Lớp Học";
    //Lấy params id truyền vào
    string id = ViewData["Id"] as string;
    string idLvl2 = ViewData["IdLvl2"] as string;
    string idLvl3 = ViewData["IdLvl3"] as string;
    string idP = ViewData["IdPage"] as string;
    string error = ViewData["Error"] as string;
    int idPage = 0;
    if (!string.IsNullOrEmpty(idP))
    {
        idPage = int.Parse(idP);
    }


    string idHomework = ViewData["IdHomework"] as string;
    Course course = Model.Courses.FirstOrDefault();
    CourseLevel2 courselvl2 = Model.CoursesLevel2.FirstOrDefault(crs => crs.CourseId2 == int.Parse(idLvl2));
    CourseLevel3 courselvl3 = new CourseLevel3();
    List<Document> docLvl3 = new List<Document>();
    List<List<Question>> questions = new List<List<Question>>();
    if (!string.IsNullOrEmpty(idLvl3))
    {
        courselvl3 = Model.CoursesLevel3.SingleOrDefault(crs => crs.CourseId3 == int.Parse(idLvl3));
        //Lấy thông tin tài liệu
        ////Lấy id tài liệu có trong bảng CourseLevel3
        if (courselvl3 != null)
        {
            //Question test
            if (!string.IsNullOrEmpty(courselvl3.Questions))
            {
                List<List<int>> listIdQuestion = JsonConvert.DeserializeObject<List<List<int>>>(courselvl3.Questions);

                foreach (List<int> lIdQuestion in listIdQuestion)
                {
                    var lQuestionToAdd = new List<Question>();
                    foreach (int idQuestion in lIdQuestion)
                    {
                        var questionToAdd = Model.Questions.SingleOrDefault(q => q.QuestionId == idQuestion);
                        questionToAdd.Answer = Model.Answers.Where(a => a.QuestionId == idQuestion).ToList();
                        lQuestionToAdd.Add(questionToAdd);
                    }
                    questions.Add(lQuestionToAdd);
                }
            }
            if (!string.IsNullOrEmpty(courselvl3.DocumentsId))
            {
                List<string> idDocs = courselvl3.DocumentsId.Split(",").ToList<string>();
                foreach (Document doc in Model.Documents)
                {
                    if (idDocs.IndexOf(doc.DocId.ToString()) != -1)
                    {
                        docLvl3.Add(doc);
                    }
                }
            }
        }
    }


}
<link rel="stylesheet" href="~/css/coursestudent.css">

<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>@course.CourseName - @courselvl2.Name</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item active">Học Viên</li>
                    <li class="breadcrumb-item active">Khóa Học</li>
                    <li class="breadcrumb-item active">@course.CourseName</li>
                    <li class="breadcrumb-item active">@courselvl2.Name</li>
                    @if (!string.IsNullOrEmpty(idLvl3))
                    {
                        <li class="breadcrumb-item active">@courselvl3.Name</li>
                    }
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
    @if (string.IsNullOrEmpty(idLvl3))
    {
        <div class="card">
            <div class="card-header">
                <h4>Mời Chọn Bài Tập Để Tham Gia</h4>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-12 col-md-12 col-lg-12 order-2 order-md-1">
                        <div class="row">
                            <div class="col-12">
                                @if (Model.CoursesLevel3.Count() < 1)
                                {
                                    <h1 class="text-center">Không có bài tập nào để hiển thị</h1>
                                }
                                @foreach (var courseLvl3 in Model.CoursesLevel3)
                                {
                                    <ul class="list-group list-group-flush">
                                        <li class="list-group-item">
                                            <div class="d-grid gap-2">
                                                <a class="btn btn-outline-primary btn-lg btn-block" style="text-overflow: ellipsis; overflow: hidden; white-space: nowrap;" href="/Student/Course/@course.CourseId/@courselvl2.CourseId2/@courseLvl3.CourseId3">@courseLvl3.Name</a>
                                            </div>
                                        </li>
                                    </ul>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    else
    {

        <div class="card card-primary card-outline">
            <div class="card-header d-flex">
                <a class="col-2 link" href="/Student/Course/@id/@idLvl2"><i class="fas fa-arrow-left"></i></a>
                <h3 class="card-title col-10 text-right">
                    <i class="fas fa-book-open"></i>&nbsp;&nbsp;&nbsp;
                    @courselvl3.Name
                </h3>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-10">
                        <div class="tab-content" id="vert-tabs-right-tabContent">
                            <div class="tab-pane fade @if (string.IsNullOrEmpty(idHomework)) { @("show active")}" id="vert-tabs-right-practice" role="tabpanel" aria-labelledby="vert-tabs-right-practice-tab">
                                @if (questions.Count < 1)
                                {
                                    <div class="col-12 text-center h1">Nichts zu zeigen!</div>
                                }
                                else
                                {
                                    @foreach (List<Question> lQues in questions)
                                    {
                                        if (questions.IndexOf(lQues) == idPage)
                                        {
                                            var listContent = JsonConvert.DeserializeObject<List<List<string>>>(courselvl3.Content);
                                            <div></div>
                                            @if (listContent[idPage].Count == 1)
                                            {
                                                <div class="col-12">
                                                    <div class="card card-primary">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Nội Dung Bài Tập</h3>
                                                        </div>
                                                        <div class="card-body">
                                                            @Html.Raw(System.Net.WebUtility.HtmlDecode(listContent[idPage][0]))
                                                        </div>
                                                    </div>
                                                </div>
                                            }

                                            <div class="question-container container-fluid" id="questionContainer_@questions.IndexOf(lQues)">
                                                @foreach (Question ques in lQues)
                                                {

                                                    <div class="col-12" style="min-height:450px">
                                                        <div class="card card-primary">
                                                            <div class="card-header">
                                                                <h3 class="card-title">
                                                                    <strong><i class="fas fa-question"></i>&nbsp;&nbsp;&nbsp;@ques.QuestionId</strong>
                                                                </h3>
                                                            </div>
                                                            <!-- /.card-header -->
                                                            <div class="card-body" style="min-height: 350px">
                                                                @if (ques.Type == "1")
                                                                {
                                                                    <div class="col-12 mb-3">
                                                                        @Html.Raw(System.Net.WebUtility.HtmlDecode(ques.Content))
                                                                    </div>
                                                                    <div class="col-12">
                                                                        <div class="form-group row text-center justify-content-center">
                                                                            @foreach (Answer ans in ques.Answer)
                                                                            {
                                                                                <div class="select-for-type-1 p-1 m-1 @((ans.Type.IndexOf("video") != -1 || ans.Type.IndexOf("image") != -1) ? "col-5" : "col-12")" id="formCheck_@(ans.QuestionId)_@(ans.AnswerId)">
                                                                                    <input class="form-check-input d-none check-input-ans" id="inputCheck_@(ans.QuestionId)_@(ans.AnswerId)" type="checkbox">
                                                                                    <div class="form-check-label text-center">
                                                                                        @if (ans.Type == "text")
                                                                                        {
                                                                                            <div class="check-lable-ans p-3" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@ans.Content</div>
                                                                                        }
                                                                                        else if (ans.Type.IndexOf("video") != -1)
                                                                                        {
                                                                                            <div class="check-lable-ans" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@(string.IsNullOrEmpty(ans.Content) ? "" : ans.Content )</div>
                                                                                            <video controls class="col-12" src="/Admin/StreamFile/@ans.DocId"></video>
                                                                                        }
                                                                                        else if (ans.Type.IndexOf("image") != -1)
                                                                                        {
                                                                                            <div class="check-lable-ans" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@(string.IsNullOrEmpty(ans.Content) ? "" : ans.Content )</div>
                                                                                            <img class="col-12" style="max-width:250px" src="/Admin/StreamFile/@ans.DocId">
                                                                                        }
                                                                                        else if (ans.Type.IndexOf("audio") != -1)
                                                                                        {
                                                                                            <div class="check-lable-ans" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@(string.IsNullOrEmpty(ans.Content) ? "" : ans.Content )</div>
                                                                                            <audio controls class="col-12" src="/Admin/StreamFile/@ans.DocId"></audio>
                                                                                        }
                                                                                    </div>
                                                                                </div>
                                                                            }
                                                                        </div>
                                                                    </div>

                                                                }
                                                                else if (ques.Type == "2")
                                                                {
                                                                    <div class="col-12 mb-3">
                                                                        @Html.Raw(System.Net.WebUtility.HtmlDecode(ques.Content))
                                                                    </div>
                                                                    <div class="col-12">
                                                                        <div class="card">
                                                                            <div class="card-header">
                                                                                <h3 class="card-title">Ihre Antwort</h3>
                                                                            </div>
                                                                            <!-- /.card-header -->
                                                                            <div class="card-body table-responsive p-0">
                                                                                <table class="table table-head-fixed">
                                                                                    <thead>
                                                                                        <tr>
                                                                                            <th style="width: 45%" class="text-center"></th>
                                                                                            <th style="width: 10%"></th>
                                                                                            <th style="width: 45%" class="text-center"></th>
                                                                                        </tr>
                                                                                    </thead>
                                                                                    <tbody class="display-answer-for-type-2" id="displayAnswerForType2_@ques.QuestionId">
                                                                                        @for (int i = 0; i < ques.Answer.Count / 2; i++)
                                                                                        {
                                                                                            <tr>
                                                                                                <td width="45%" id="displayAnswerForType2_left_@(ques.QuestionId)_index@(i)"></td>
                                                                                                <td width="10%"></td>
                                                                                                <td width="45%" id="displayAnswerForType2_right_@(ques.QuestionId)_index@(i)"></td>
                                                                                            </tr>
                                                                                        }
                                                                                    </tbody>
                                                                                </table>
                                                                            </div>
                                                                            <!-- /.card-body -->
                                                                        </div>
                                                                        <div class="form-group text-center">
                                                                            @{
                                                                                var countForType2 = 0;
                                                                                var displayAnswerForType2 = "";
                                                                                List<Answer> listAnswer1 = new List<Answer>();
                                                                                List<Answer> listAnswer2 = new List<Answer>();
                                                                                for (int i = 0; i < ques.Answer.Count; i++)
                                                                                {
                                                                                    if (i % 2 == 0)
                                                                                    {
                                                                                        listAnswer1.Add(ques.Answer.ElementAt(i));

                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        listAnswer2.Add(ques.Answer.ElementAt(i));

                                                                                    }
                                                                                }
                                                                                listAnswer1 = listAnswer1.OrderBy(i => Guid.NewGuid()).ToList();
                                                                                listAnswer2 = listAnswer2.OrderBy(i => Guid.NewGuid()).ToList();
                                                                                List<Answer> listAnswerForType2 = new List<Answer>();
                                                                                for (int i = 0; i < ques.Answer.Count / 2; i++)
                                                                                {
                                                                                    listAnswerForType2.Add(listAnswer1[i]);
                                                                                    listAnswerForType2.Add(listAnswer2[i]);
                                                                                }
                                                                            }
                                                                            @foreach (Answer ans in listAnswerForType2)
                                                                            {
                                                                                countForType2 += 1;
                                                                                if (countForType2 == 1)
                                                                                {
                                                                                    if (ans.Type == "text")
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div  class='container-fluid row mb-1'><div class='select-for-type-2 p-2 col-5 select-left-for-type-2 align-items-center d-flex' id='selectLeftForType2_{ans.QuestionId}_{ans.AnswerId}' style='min-height: 180px'><div class='col-12 font-weight-bold text-center' >{ans.Content}</div></div><div class='col-2'></div>";
                                                                                    }
                                                                                    else if (ans.Type.IndexOf("video") != -1)
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='container-fluid row mb-1'><div class='select-for-type-2 p-2 col-5 select-left-for-type-2 d-flex justify-content-center align-items-center' id='selectLeftForType2_{ans.QuestionId}_{ans.AnswerId}'><video controls class='col-12'src='/Admin/StreamFile/{ans.DocId}'></video></div><div class='col-2'></div>";
                                                                                    }
                                                                                    else if (ans.Type.IndexOf("image") != -1)
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='container-fluid row mb-1'><div class='select-for-type-2 p-2 col-5 select-left-for-type-2 d-flex justify-content-center align-items-center' id='selectLeftForType2_{ans.QuestionId}_{ans.AnswerId}' ><img src='/Admin/StreamFile/{ans.DocId}' class='col-12'/></div><div class='col-2'></div>";
                                                                                    }
                                                                                    else if (ans.Type.IndexOf("audio") != -1)
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='container-fluid row mb-1'><div class='select-for-type-2 p-2 col-5 select-left-for-type-2 d-flex justify-content-center align-items-center' id='selectLeftForType2_{ans.QuestionId}_{ans.AnswerId}' ><audio controls class='col-5 select-right-for-type-2' id='selectRightForType2_{ans.QuestionId}_{ans.AnswerId}' src='/Admin/StreamFile/{ans.DocId}'></audio></div><div class='col-2'></div>";
                                                                                    }
                                                                                }
                                                                                if (countForType2 == 2)
                                                                                {
                                                                                    if (ans.Type == "text")
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='select-for-type-2 p-2 col-5 select-right-for-type-2 align-items-center d-flex' id='selectRightForType2_{ans.QuestionId}_{ans.AnswerId}'><div class='col-12 font-weight-bold text-center' style='min-height: 180px'>{ans.Content}</div></div></div>";
                                                                                    }
                                                                                    else if (ans.Type.IndexOf("video") != -1)
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='select-for-type-2 p-2 col-5 select-right-for-type-2 d-flex justify-content-center align-items-center' id='selectRightForType2_{ans.QuestionId}_{ans.AnswerId}'><video controls class='col-12' src='/Admin/StreamFile/{ans.DocId}'></video></div></div>";
                                                                                    }
                                                                                    else if (ans.Type.IndexOf("image") != -1)
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='select-for-type-2 p-2 col-5 select-right-for-type-2 d-flex justify-content-center align-items-center' id='selectRightForType2_{ans.QuestionId}_{ans.AnswerId}'><img src='/Admin/StreamFile/{ans.DocId}' class='col-12'/></div></div>";
                                                                                    }
                                                                                    else if (ans.Type.IndexOf("audio") != -1)
                                                                                    {
                                                                                        displayAnswerForType2 += $"<div class='select-for-type-2 p-2 col-5 select-right-for-type-2 d-flex justify-content-center align-items-center' id='selectRightForType2_{ans.QuestionId}_{ans.AnswerId}'><audio controls class='col-12' src='/Admin/StreamFile/{ans.DocId}'></audio></div></div>";
                                                                                    }
                                                                                    @Html.Raw(System.Net.WebUtility.HtmlDecode(displayAnswerForType2))
                                                                                    countForType2 = 0;
                                                                                    displayAnswerForType2 = "";
                                                                                }
                                                                            }
                                                                        </div>
                                                                    </div>
                                                                }
                                                                else if (ques.Type == "3")
                                                                {
                                                                    <div class="col-12 mb-3" style=" min-height: 300px">
                                                                        @{
                                                                            var quesContentDisplayForType_3 = ques.Content;
                                                                        }
                                                                        @foreach (Answer ans in ques.Answer)
                                                                        {
                                                                            quesContentDisplayForType_3 = System.Text.RegularExpressions.Regex.Replace(quesContentDisplayForType_3, @"\[" + ans.AnswerId.ToString() + @"\]", $"<input class='input-text-for-type-3 text-center' autocomplete='off' placeholder='...............' id='inputTextForType3_{ans.QuestionId}_{ans.AnswerId}' type='text'/>");
                                                                        }
                                                                        @Html.Raw(System.Net.WebUtility.HtmlDecode(quesContentDisplayForType_3))
                                                                    </div>
                                                                }
                                                                else if (ques.Type == "4")
                                                                {
                                                                    <div class="col-12 mb-3" style=" min-height: 300px">
                                                                        @Html.Raw(System.Net.WebUtility.HtmlDecode(ques.Content))
                                                                    </div>
                                                                    <div class="col-12">
                                                                        <div class="form-group">
                                                                            <label for="inputTextForType4_@ques.QuestionId">Trả Lời:</label>
                                                                            <textarea class="form-control input-text-for-type-4" id="inputTextForType4_@ques.QuestionId" rows="10"></textarea>
                                                                        </div>
                                                                    </div>
                                                                }
                                                                else if (ques.Type == "5")
                                                                {
                                                                    <div class="col-12 mb-3" style="min-height: 300px">
                                                                        @Html.Raw(System.Net.WebUtility.HtmlDecode(ques.Content))
                                                                    </div>
                                                                    <div class="col-12">
                                                                        <div class="form-group">
                                                                            <label for="inputTextForType5_@ques.QuestionId">Trả Lời:</label>
                                                                            <textarea class="form-control input-text-for-type-4" id="inputTextForType5_@ques.QuestionId" rows="10"></textarea>
                                                                        </div>
                                                                    </div>
                                                                }
                                                                else if (ques.Type == "6")
                                                                {
                                                                    <div class="col-12 mb-3">
                                                                        @Html.Raw(System.Net.WebUtility.HtmlDecode(ques.Content))
                                                                    </div>
                                                                    <div class="col-12">
                                                                        <div class="form-group row text-center justify-content-center">
                                                                            @foreach (Answer ans in ques.Answer)
                                                                            {
                                                                                <div class="select-for-type-6 p-1 m-1 @((ans.Type.IndexOf("video") != -1 || ans.Type.IndexOf("image") != -1) ? "col-5" : "col-12")" id="formCheck_@(ans.QuestionId)_@(ans.AnswerId)">
                                                                                    <input class="form-check-input d-none check-input-ans" id="inputCheck_@(ans.QuestionId)_@(ans.AnswerId)" type="checkbox">
                                                                                    <div class="form-check-label text-center">
                                                                                        @if (ans.Type == "text")
                                                                                        {
                                                                                            <div class="check-lable-ans-type-6 p-3" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@ans.Content</div>
                                                                                        }
                                                                                        else if (ans.Type.IndexOf("video") != -1)
                                                                                        {
                                                                                            <div class="check-lable-ans-type-6" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@(string.IsNullOrEmpty(ans.Content) ? "" : ans.Content )</div>
                                                                                            <video controls class="col-12" src="/Admin/StreamFile/@ans.DocId" controlsList="nodownload"></video>
                                                                                        }
                                                                                        else if (ans.Type.IndexOf("image") != -1)
                                                                                        {
                                                                                            <div class="check-lable-ans-type-6" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@(string.IsNullOrEmpty(ans.Content) ? "" : ans.Content )</div>
                                                                                            <img class="col-12" style="max-width:250px" src="/Admin/StreamFile/@ans.DocId">
                                                                                        }
                                                                                        else if (ans.Type.IndexOf("audio") != -1)
                                                                                        {
                                                                                            <div class="check-lable-ans-type-6" id="lableCheck_@(ans.QuestionId)_@(ans.AnswerId)" style="cursor:pointer">@(string.IsNullOrEmpty(ans.Content) ? "" : ans.Content )</div>
                                                                                            <audio controls class="col-12" src="/Admin/StreamFile/@ans.DocId" controlsList="nodownload"></audio>
                                                                                        }
                                                                                    </div>
                                                                                </div>
                                                                            }
                                                                        </div>
                                                                    </div>
                                                                }
                                                                <div class="row col-12">
                                                                    <div class="col-6 text-center"><button class="btn btn-outline-secondary btn-block see-answer-btn" id="seeAnswerBtn_@(ques.QuestionId)_@courselvl3.CourseId3" style="border-radius:20px" disabled>Siehe Antworten</button></div>
                                                                    <div class="col-6 text-center"><button class="btn btn-outline-success btn-block check-answer-btn" id="checkAnswerBtn_@(ques.QuestionId)_@courselvl3.CourseId3" style="border-radius:20px">Kontrolliere Deine Antworten</button></div>
                                                                </div>
                                                            </div>
                                                            <!-- /.card-body -->
                                                        </div>
                                                    </div>

                                                }

                                                <div class="col-12 d-flex justify-content-center align-items-center mt-4">
                                                    @if (idPage >= 0)
                                                    {
                                                        if (idPage == 0)
                                                        {
                                                            if (idPage != (questions.Count - 1))
                                                            {
                                                                <a href="/Student/Course/@id/@idLvl2/@idLvl3/@(idPage + 1)" class="btn btn-primary col-5"><i class="fas fa-angle-right fa-2x p-1"></i></a>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <a href="/Student/Course/@id/@idLvl2/@idLvl3/@(idPage-1)" class="btn btn-primary col-5 mr-2"><i class="fas fa-angle-left fa-2x p-1"></i></a>
                                                            if (idPage != (questions.Count - 1))
                                                            {

                                                                <a href="/Student/Course/@id/@idLvl2/@idLvl3/@(idPage+1)" class="btn btn-primary col-5 ml-2"><i class="fas fa-angle-right fa-2x p-1"></i></a>
                                                            }
                                                        }

                                                    }
                                                </div>
                                            </div>
                                        }
                                    }
                                }
                            </div>
                            <div class="tab-pane fade" id="vert-tabs-right-document" role="tabpanel" aria-labelledby="vert-tabs-right-document-tab">
                                <div class="card">
                                    <div class="card-header">
                                        <h3 class="card-title">Danh sách tài liệu</h3>

                                        <div class="card-tools">
                                            <div class="input-group input-group-sm" style="width: 200px;">
                                                <input type="text" id="searchDocInput" name="table_search_@idLvl3" class="form-control float-right" placeholder="Tìm kiếm theo tên">

                                                <div class="input-group-append">
                                                    <button type="submit" class="btn btn-default" disabled><i class="fas fa-search"></i></button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /.card-header -->
                                    <div class="card-body table-responsive p-0" style="height: 40vh;">
                                        <table class="table table-head-fixed" style="table-layout: fixed; border-collapse: collapse;">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Tên Tài Liệu</th>
                                                    <th>Định Dạng</th>
                                                    <th>Ngày Tạo</th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody id="docTableBody">
                                                @foreach (Document doc in docLvl3)
                                                {
                                                    <tr>
                                                        <td>@doc.DocId</td>
                                                        <td style="overflow: hidden; white-space: nowrap; text-overflow: ellipsis;">@doc.DocName</td>
                                                        <td style="overflow: hidden; white-space: nowrap; text-overflow: ellipsis;">@doc.Type</td>
                                                        <td>@doc.CreatedDate</td>
                                                        @if (doc.Type.IndexOf("application") != -1)
                                                        {
                                                            <td style="width: 12%"><a class="btn btn-success col-12" href="/Admin/File/@doc.DocId">Tải</a></td>
                                                        }
                                                        else
